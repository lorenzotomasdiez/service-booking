# AFIP Mock Server Configuration

# Server Port
AFIP_MOCK_PORT=3002

# Database
AFIP_MOCK_DB_PATH=./data/afip.db

# Default POS (Punto de Venta)
AFIP_MOCK_DEFAULT_POS=1

# Logging
LOG_LEVEL=info
NODE_ENV=development

# Enable/Disable Features
ENABLE_SWAGGER=true
ENABLE_CORS=true

# Response Simulation
SIMULATE_DELAYS=false
SIMULATE_ERRORS=false
ERROR_RATE=0.05
